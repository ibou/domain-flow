{
    "name": "tboileau/domain-flow",
    "description": "PHP >= 7.4 library, manage the flow of control of your domain in clean architecture",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TBoileau",
            "email": "t-boileau@email.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "TBoileau\\DomainFlow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TBoileau\\DomainFlow\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "7.4.*",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
